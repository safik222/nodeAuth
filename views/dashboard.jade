extends base

block vars
	- var title = 'Dashboard'

block body
	div.top
		div.title
			h1 dashboard
	div.main
		p.
			Welcome to your dashboard! You are now logged in.

		h2 User Information
		p First Name: #{user.firstName}
		p Last Name:  #{user.lastName}
		p Email:      #{user.email}
		p Data:       #{user.data}
		p
			a(href="/logout")
				button(type="button").
					Logout